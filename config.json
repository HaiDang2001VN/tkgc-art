{
    "models": {
        "DGT": {
            "d_model": 64,
            "nhead": 8,
            "num_layers": 12,
            "intermediate_layers": {
                "8": 0.1,
                "10": 0.2,
                "12": 0.7
            }
        },
        "PGT": {
            "d_model": 64,
            "nhead": 4,
            "num_layers": 4,
            "current_inclusive": false
        }
    },
    "training": {
        "lr": 0.001,
        "batch_size": 8,
        "num_workers": 8,
        "k_hops": 2,
        "num_epochs": 3,
        "devices": "auto",
        "accelerator": "auto",
        "persistent_workers": true,
        "pin_memory": true,
        "strategy": null,
        "precision": 32,
        "predictive": false,
        "log_freq": 5,
        "log_flush": 50,
        "fan_out": 30,
        "degree_sort": "decreasing"
    },
    "data": {
        "name": "ogbl-collab",
        "path": "../data",
        "random_seed": 42,
        "edge_time": "year",
        "directionality": "bi",
        "neg_ratio": 1
    }
}