{
    "models": {
        "DGT": {
            "d_model": 64,
            "nhead": 8,
            "num_layers": 12,
            "intermediate_layers": {
                "7": 0.1,
                "9": 0.2,
                "11": 0.7
            },
            "dim_ffn": 128,
            "activation": "gelu"
        },
        "PGT": {
            "d_model": 64,
            "nhead": 4,
            "num_layers": 8,
            "current_inclusive": false,
            "dim_ffn": 128,
            "activation": "gelu"
        }
    },
    "training": {
        "lr": 0.01,
        "batch_size": 128,
        "num_workers": 8,
        "k_hops": 2,
        "max_degree": 10,
        "num_epochs": 50,
        "devices": "cpu",
        "accelerator": "auto",
        "persistent_workers": true,
        "pin_memory": true,
        "strategy": null,
        "precision": 32,
        "predictive": true,
        "similarity_type": "cosine",
        "log_freq": 5,
        "log_flush": 50,
        "fan_out": 25,
        "degree_sort": "decreasing",
        "ckpt_dir": "checkpoints",
        "ckpt_name": "model",
        "ckpt_freq": 1,
        "ckpt_keep": 1,
        "wandb_project": "temporal-graph-learning",
        "wandb_group": "similarity_comparison",
        "wandb_tags": ["temporal", "graph", "transformer"],
        "wandb_log_model": "all",
        "steps_per_checkpoint": 10,
        "log_dir": "model"
    },
    "data": {
        "name": "ogbl-collab",
        "path": "data",
        "random_seed": 42,
        "edge_time": "year",
        "directionality": "bi",
        "neg_multiplier": 0
    }
}